    "date-fns": "^4.1.0",
    "isbot": "^4.1.0",
    "jotai": "^2.12.5",
    "lucide-react": "^0.516.0",
    "openai": "^5.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.58.1",
    "react-hot-toast": "^2.5.2",
    "supabase": "^2.24.3",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "zod": "^3.25.57"
