{
  "name": "squire-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "db:generate-types": "supabase gen types typescript --project-id YOUR_PROJECT_ID > app/shared/types/database.ts",
    "docs:dev": "echo 'Documentation available in /docs folder'",
    "format": "prettier --write ."
  },
  "dependencies": {
